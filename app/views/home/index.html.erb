      <div class="col s12 hero-caption">
        <h1>WE ARE GETTING MARRIED</h1>
        <div class="col s6 center-block">
          <div class="col s12 m6">
            <a class="waves-effect waves-light btn-large home-btns" href="#registry">Registry</a>
          </div>
          <div class="col s12 m6">
            <%if user_signed_in?%>
            <%= link_to "Your RSVPs", guests_path, class: "waves-effect waves-light btn-large home-btns"%>
            <%else%>
            <%= link_to "RSVP", new_guest_path, class: "waves-effect waves-light btn-large home-btns"%>
            <%end%>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>

